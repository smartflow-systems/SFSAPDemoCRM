{
  "theme": {
    "name": "SFS Family Theme",
    "version": "1.0.0",
    "colors": {
      "primary": {
        "black": "#0D0D0D",
        "brown": "#3B2F2F",
        "gold": "#FFD700",
        "goldHover": "#E6C200",
        "beige": "#F5F5DC"
      },
      "semantic": {
        "success": "#10B981",
        "warning": "#F59E0B",
        "error": "#EF4444",
        "info": "#3B82F6"
      },
      "text": {
        "primary": "#F5F5DC",
        "secondary": "#D4C5A0",
        "muted": "#A89968",
        "gold": "#FFD700"
      }
    },
    "spacing": {
      "xs": "0.25rem",
      "sm": "0.5rem",
      "md": "1rem",
      "lg": "1.5rem",
      "xl": "2rem",
      "2xl": "3rem",
      "3xl": "4rem"
    },
    "borderRadius": {
      "sm": "0.375rem",
      "md": "0.5rem",
      "lg": "0.75rem",
      "xl": "1rem",
      "2xl": "1.5rem",
      "full": "9999px"
    },
    "elevation": {
      "low": "0 2px 8px rgba(255, 215, 0, 0.1)",
      "medium": "0 4px 16px rgba(255, 215, 0, 0.15)",
      "high": "0 8px 32px rgba(255, 215, 0, 0.2)",
      "glow": "0 0 24px rgba(255, 215, 0, 0.3)"
    },
    "animation": {
      "duration": {
        "fast": "150ms",
        "normal": "300ms",
        "slow": "500ms"
      },
      "easing": {
        "cricketFlow": "cubic-bezier(0.4, 0.0, 0.2, 1)",
        "bounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
        "smooth": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      }
    },
    "glassmorphism": {
      "blur": "12px",
      "opacity": "0.1",
      "borderOpacity": "0.2"
    },
    "circuitFlow": {
      "enabled": true,
      "particleCount": 80,
      "speed": 0.3,
      "lineWidth": 1,
      "nodeRadius": 3,
      "connectionDistance": 150,
      "goldOpacity": 0.6
    }
  }
}
